package inheritance.airplane;

public class SuperSonicAirPlain extends AirPlane{
	
	
	//비행모드 1- 일반비행, 2-초음속비행
	int flyMode = 1;
	
	@Override //메서드 재정의
	public void fly() {
		//조건문 - 비행기가 초음속 비행합니다.
		if(flyMode == 2) {
			System.out.println("비행기가 초음속 비행합니다.");
		}else {
			super.fly();
		}
		
		
	}

}
